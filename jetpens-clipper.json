{
	"schemaVersion": "0.1.0",
	"name": "Jetpens",
	"behavior": "create",
	"noteContentFormat": "# {{schema:@product:name}}\n\n`=embed(this.image)`\n\n{{selector:.pswp-photo img?src|unique|image|list}}\n\n## Description\n\n{{selectorHtml:.description|markdown}}\n\n## Product Specifications\n\n{{selectorHtml:#product-specifications-container|markdown}}",
	"properties": [
		{
			"name": "dateCreated",
			"value": "{{time|date:\\\"YYYY-MM-DDTHH:mm:ssZ\\\"}}",
			"type": "datetime"
		},
		{
			"name": "url",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "title",
			"value": "{{schema:@product:name}}",
			"type": "text"
		},
		{
			"name": "price",
			"value": "{{schema:@product:offers.price}} {{schema:@product:offers.priceCurrency}}",
			"type": "text"
		},
		{
			"name": "tags",
			"value": "clippings, products,schemaProduct,stationary",
			"type": "multitext"
		},
		{
			"name": "brand",
			"value": "{{schema:@product:brand}}",
			"type": "text"
		},
		{
			"name": "image",
			"value": "{{selector:.pswp-photo img?src|unique|first|split:\\\"/\\\"|last|split:\\\"?\\\"|first|wikilink}}",
			"type": "text"
		}
	],
	"triggers": [
		"https://www.jetpens.com/"
	],
	"noteNameFormat": "{{schema:@product:name|safe_name}}",
	"path": "3 Ressurser/Ã˜nskeliste/{{domain}}"
}