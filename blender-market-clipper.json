{
	"schemaVersion": "0.1.0",
	"name": "Blender Market",
	"behavior": "create",
	"noteContentFormat": "# {{schema:@Product:name}}\n\n{{selector:.swiper-wrapper img?src|image|list}}\n\n{{selectorHtml:.product-info|markdown}}\n\n{{selectorHtml:#tab-content-main|remove_html:(\"iframe\")|markdown}}",
	"properties": [
		{
			"name": "dateCreated",
			"value": "{{time|date:\\\"YYYY-MM-DDTHH:mm:ssZ\\\"}}",
			"type": "datetime"
		},
		{
			"name": "url",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "title",
			"value": "{{schema:@Product:name}}",
			"type": "text"
		},
		{
			"name": "price",
			"value": "{{schema:@Product:offers.price}} {{schema:@Product:offers.priceCurrency}}",
			"type": "text"
		},
		{
			"name": "tags",
			"value": "clippings, products, blender, schemaProduct, {{selector:.product-meta a|last|lower}}",
			"type": "multitext"
		},
		{
			"name": "artists",
			"value": "{{selector:.product-meta a|first}}",
			"type": "multitext"
		},
		{
			"name": "image",
			"value": "{{image}}",
			"type": "text"
		}
	],
	"triggers": [
		"https://blendermarket.com/",
		"https://superhivemarket.com/"
	],
	"noteNameFormat": "{{title|safe_name}}",
	"path": "3 Ressurser/10 Personlig/12 Ã˜nskeliste/{{url|split:\"/\"|nth:3|join:\"\"}}/{{title|safe_name}}"
}