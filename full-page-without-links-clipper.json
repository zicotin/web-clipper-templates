{
	"schemaVersion": "0.1.0",
	"name": "Full page without links",
	"behavior": "create",
	"noteContentFormat": "# {{title|strip_tags|strip_md}}\n\n{{contentHtml|remove_tags:(\"a\")|markdown|blockquote}}",
	"properties": [
		{
			"name": "title",
			"value": "{{title}}",
			"type": "text"
		},
		{
			"name": "url",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "authors",
			"value": "{{author}}",
			"type": "multitext"
		},
		{
			"name": "datePublished",
			"value": "{{published}}",
			"type": "text"
		},
		{
			"name": "dateCreated",
			"value": "{{time|date:\\\"YYYY-MM-DDTHH:mm:ssZ\\\"}}",
			"type": "datetime"
		},
		{
			"name": "description",
			"value": "{{description}}",
			"type": "text"
		},
		{
			"name": "tags",
			"value": "clippings",
			"type": "multitext"
		}
	],
	"triggers": [],
	"noteNameFormat": "{{title}}",
	"path": "0 Innboks"
}