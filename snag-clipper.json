{
	"schemaVersion": "0.1.0",
	"name": "Snag",
	"behavior": "create",
	"noteContentFormat": "# {{selector:h1}}\n\n{{selector:.image-label-container img?src|unique|slice:0,5|image|list|replace:(\"![](//\":\"![](https://\")|replace:(\"/\\?.*\\)/g\":\")\")}}\n\n{{selectorHtml:.product-block__description|markdown|replace:(\"/Share your Snags\\: .*/g\":\"\")}}",
	"properties": [
		{
			"name": "brand",
			"value": "Snag",
			"type": "text"
		},
		{
			"name": "size",
			"value": "{{selector:variant-selects legend strong|join:\\\" \\\"}}",
			"type": "text"
		},
		{
			"name": "price",
			"value": "{{schema:@ProductGroup:hasVariant[0].offers.price}} {{schema:@ProductGroup:hasVariant[0].offers.priceCurrency}}",
			"type": "text"
		},
		{
			"name": "title",
			"value": "{{selector:h1}}",
			"type": "text"
		},
		{
			"name": "url",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "tags",
			"value": "product, wardrobe",
			"type": "multitext"
		},
		{
			"name": "icon",
			"value": "shirt",
			"type": "text"
		}
	],
	"triggers": [
		"https://snagtights.eu/"
	],
	"noteNameFormat": "{{selector:h1|safe_name}}",
	"path": "3 Ressurser/Ã˜nskeliste/{{domain}}"
